{
  "version": 2,
  "name": "eal-time-chat-app-using-socket-io",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGO_PASSWORD": "@mongo_password"
  }
}
